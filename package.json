{
	"name": "@ctx-core/rollup",
	"version": "13.1.74",
	"description": "ctx-core rollup",
	"keywords": [
		"ctx-core",
		"rollup"
	],
	"homepage": "https://github.com/ctx-core/rollup#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/rollup/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/rollup.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"rollup-cmd": "./bin/rollup-cmd.mjs",
		"script-ts": "./bin/script-ts.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "^26.8.14",
		"@ctx-core/cli-args": "^11.0.63",
		"@ctx-core/pipe": "^6.0.42",
		"@ctx-core/queue": "^7.3.4",
		"chokidar": "^3.5.3",
		"dir-glob": "^3.0.1",
		"fs-extra": "^11.1.0",
		"globby": "^13.1.3",
		"minimist": "^1.2.7",
		"nodemon": "^2.0.20",
		"path-type": "^5.0.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.60",
		"@swc/core": "^1.3.32",
		"@types/fs-extra": "^11.0.1",
		"rimraf": "^4.1.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"svelte": "./lib/index.js",
	"sideEffects": false
}