{
	"name": "@ctx-core/rollup",
	"version": "11.0.13",
	"description": "ctx-core rollup",
	"keywords": [
		"ctx-core",
		"rollup"
	],
	"homepage": "https://github.com/ctx-core/rollup#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/rollup/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/rollup/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^21.2.9",
		"@ctx-core/cli-args": "workspace:^8.0.17",
		"@ctx-core/pipe": "workspace:^4.0.2",
		"@ctx-core/queue": "workspace:^4.0.2",
		"esm": "^3.2.25",
		"globby": "^11.0.3",
		"minimist": "^1.2.5"
	},
	"devDependencies": {
		"chokidar": "^3.5.1",
		"nodemon": "^2.0.7",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
